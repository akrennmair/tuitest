example script file:

config:
	app_under_test	newsbeuter -c cache -C /dev/null -u url.txt
	term_size		80x25

init:
	run_cmd rm cache

end:
	log_debug "finished testing."

script:
	press_key 'a'
	press_key 126
	press_key CURSOR_DOWN
	wait 3000
	expect 1 1 "newsbeuter 1.1"
	expect_soft 1 25 "q:Quit "
	call foo

foo:
	key 'q'
